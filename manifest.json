{
    "manifest_version": 3,
    "name": "Keep Tabs",
    "description": "Keep tabs on your tabs. Automatically close tabs you haven't visited in a while, which aren't pinned and aren't in a tab group.",
    "version": "0.0.1",
    "permissions": ["alarms", "storage", "tabs", "tabGroups"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/clock-16.png",
        "32": "images/clock-32.png",
        "48": "images/clock-48.png",
        "128": "images/clock-128.png"
    }
}
